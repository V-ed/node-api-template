{
  "name": "node-api-backend",
  "version": "0.0.1",
  "description": "",
  "main": "dist/main.ts",
  "scripts": {
    "start": "ts-node dist/main.ts",
    "dev": "ts-node -r tsconfig-paths/register src/main.ts",
    "watch": "nodemon src/main.ts",
    "test": "jest --detectOpenHandles",
    "coverage:test": "jest --detectOpenHandles --coverage",
    "build": "gulp build",
    "build:clean": "gulp cleanbuild",
    "debug": "node -r ts-node/register --inspect-brk src/main.ts",
    "seed": "prisma db seed --preview-feature",
    "init": "npm-run-all -s init:empty seed",
    "init:empty": "gulp init",
    "format": "eslint --fix \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:tests": "eslint \"tests/**/*.ts\"",
    "coverage:pr": "jest --detectOpenHandles --silent --coverage --json --outputFile=\"jest.results.json\""
  },
  "author": "Guillaume Marcoux",
  "license": "MIT",
  "devDependencies": {
    "@esbuild-plugins/tsconfig-paths": "0.0.4",
    "@getbigger-io/prisma-fixtures-cli": "1.0.9",
    "@types/body-parser": "1.19.0",
    "@types/express": "4.17.11",
    "@types/gulp": "4.0.8",
    "@types/gulp-rename": "2.0.0",
    "@types/gulp-replace": "0.0.31",
    "@types/jest": "26.0.20",
    "@types/json5": "2.2.0",
    "@types/node": "14.14.31",
    "@types/recursive-readdir": "2.2.0",
    "@types/socket.io": "2.1.13",
    "@types/supertest": "2.0.10",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "del": "6.0.0",
    "eslint": "7.21.0",
    "gulp": "4.0.2",
    "gulp-esbuild": "0.5.2",
    "gulp-rename": "2.0.0",
    "gulp-replace": "1.0.0",
    "gulp-ts-alias": "1.3.0",
    "gulp-typescript": "6.0.0-alpha.1",
    "jest": "26.6.3",
    "json5": "2.2.0",
    "nodemon": "2.0.7",
    "npm-run-all": "4.1.5",
    "prisma": "2.17.0",
    "supertest": "6.1.3",
    "ts-jest": "26.5.2",
    "ts-node": "9.1.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.2.2"
  },
  "dependencies": {
    "@prisma/client": "^2.17.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.10.0",
    "recursive-readdir": "^2.2.2",
    "socket.io": "^3.1.2",
    "ws": "^7.4.3"
  }
}
